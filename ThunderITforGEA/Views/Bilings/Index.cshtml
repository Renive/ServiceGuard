@model IEnumerable<ThunderITforGEA.Models.Biling>

@{
    ViewBag.Title = "Index";
}

<h2>Bilingi</h2>
@Html.ActionLink("Eksport do PDF", "Edit", "SerialPort", new { id = @ViewBag.User_Id }, new { @class = "btn btn-default", @style = "margin-bottom:5px;" })
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tresc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceGuard.serial_number)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tresc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceGuard.serial_number)
        </td>
     
    </tr>
}

</table>
